rt OrderListScreen from \"./screens/OrderListScreen\";\n\nfunction App() {\n  const cart = useSelector((state) => state.cart);\n  const { cartItems } = cart;\n  const userSignin = useSelector((state) => state.userSignin);\n  const { userInfo } = userSignin;\n  const dispatch = useDispatch();\n  const signoutHandler = () => {\n    dispatch(signout());\n  };\n  return (\n    <BrowserRouter>\n      <div className=\"grid-container\">\n        <header className=\"row\">\n          <div class=\"row\">\n            <img class=\"small\" src=\"images/logo.png\"></img>\n            <Link className=\"artist\" to=\"/\">\n              BLM Art Gallery\n            </Link>\n          </div>\n          <div>\n            <Link to=\"/cart\">\n              Cart\n              {cartItems.length > 0 && (\n                <span className=\"badge\">{cartItems.length}</span>\n              )}\n            </Link>\n            {userInfo ? (\n              <div className=\"dropdown\">\n                <Link to=\"#\">\n                  {userInfo.name} <i className=\"fa fa-caret-down\"></i>{\" \"}\n                </Link>\n                <ul className=\"dropdown-content\">\n                  <li>\n                    <Link to=\"/profile\">User Profile</Link>\n                  </li>\n                  <li>\n                    <Link to=\"/orderhistory\">Order History</Link>\n                  </li>\n                  <li>\n                    <Link to=\"#signout\" onClick={signoutHandler}>\n                      Sign Out\n                    </Link>\n                  </li>\n                </ul>\n              </div>\n            ) : (\n              <Link to=\"/signin\">Sign In</Link>\n            )}\n            {userInfo && userInfo.isAdmin && (\n              <div className=\"dropdown\">\n                <Link to=\"#admin\">\n                  Admin <i className=\"fa fa-caret-down\"></i>\n                </Link>\n                <ul className=\"dropdown-content\">\n                  <li>\n                    <Link to=\"/dashboard\">Dashboard</Link>\n                  </li>\n                  <li>\n                    <Link to=\"/productlist\">Products</Link>\n                  </li>\n                  <li>\n                    <Link to=\"/orderlist\">Orders</Link>\n                  </li>\n                  <li>\n                    <Link to=\"/userlist\">Users</Link>\n                  </li>\n                </ul>\n              </div>\n            )}\n          </div>\n        </header>\n        <main>\n          <Route path=\"/cart/:id?\" component={CartScreen}></Route>\n          <Route path=\"/product/:id\" component={ProductScreen} exact></Route>\n          <Route\n            path=\"/product/:id/edit\"\n            component={ProductEditScreen}\n            exact\n          ></Route>\n          <Route path=\"/signin\" component={SigninScreen}></Route>\n          <Route path=\"/register\" component={RegisterScreen}></Route>\n          <Route path=\"/organization\" component={ShippingAddressScreen}></Route>\n          <Route path=\"/payment\" component={PaymentMethodScreen}></Route>\n          <Route path=\"/placeorder\" component={PlaceOrderScreen}></Route>\n          <Route path=\"/order/:id\" component={OrderScreen}></Route>\n          <Route path=\"/orderhistory\" component={OrderHistoryScreen}></Route>\n          <PrivateRoute\n            path=\"/profile\"\n            component={ProfileScreen}\n          ></PrivateRoute>\n          <AdminRoute\n            path=\"/productlist\"\n            component={ProductListScreen}\n          ></AdminRoute>\n          <AdminRoute\n            path=\"/orderlist\"\n            component={OrderListScreen}\n          ></AdminRoute>\n          <Route path=\"/\" component={HomeScreen} exact></Route>\n        </main>\n        <footer className=\"row center\">All right reserved</footer>\n      </div>\n    </BrowserRouter>\n  );\n}\n\nexport default App;\n","/Users/glam/artgallery/client/src/reportWebVi