{
  "name": "artgallery",
  "type": "module",
  "version": "1.0.0",
  "description": "a digital art gallery",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "client": "npm run start --prefix client",
    "server": "nodemon --exec babel-node server.js",
